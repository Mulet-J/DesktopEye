<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:screenCapture="clr-namespace:DesktopEye.Common.Application.Views.ScreenCapture"
        mc:Ignorable="d" d:DesignWidth="1400" d:DesignHeight="800"
        x:Class="DesktopEye.Common.Application.Views.ScreenCapture.ScreenCaptureActionsWindow"
        Title="DesktopEye - Analyse d'image"
        Icon="/Application/Assets/avalonia-logo.ico"
        MinWidth="1000" MinHeight="600"
        SystemDecorations="Full"
        TransparencyLevelHint="AcrylicBlur"
        Background="{DynamicResource SystemControlBackgroundAltHighBrush}">

    <screenCapture:ScreenCaptureActionsView />
</Window>